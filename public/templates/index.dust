{>"layouts/master" /}

{<body}
  {#songs}
    <div class="col-lg-3 song">
      <img class="imageframe" src="/images/{.cover}">
      <h4>{.title}</h4>
      <p>{.truncText}</p>
      <div class="price">Buy it for <span>${.price}</span></div>
